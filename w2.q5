class Solution {
public:
    string defangIPaddr(string s) {
        string s2;
        for(int i=0;i<s.length();i++)
        {
            if(s[i]=='.')
            {
                s2+="[.]";
            }
            else
            {
                s2+=s[i];
            }
        }
        return s2;
    }
};
